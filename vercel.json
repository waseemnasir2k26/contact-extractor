{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/extract",
      "destination": "/api/extract"
    },
    {
      "source": "/api/health",
      "destination": "/api/health"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
